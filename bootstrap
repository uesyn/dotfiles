setup_kubectx() {
  info "Installing kubectx and kubens..."
  version=v0.9.5
  kubectx_key="kubectx_${version}"
  kubens_key="kubens_${version}"

  download_if_linux_arm64 ${kubectx_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubectx_${version}_linux_arm64.tar.gz
  download_if_linux_amd64 ${kubectx_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubectx_${version}_linux_x86_64.tar.gz
  download_if_darwin_arm64 ${kubectx_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubectx_${version}_darwin_arm64.tar.gz
  download_if_darwin_amd64 ${kubectx_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubectx_${version}_darwin_x86_64.tar.gz
  kubectx_cache_file=$(get_cache ${kubectx_key})
  (
    cd ${OPT_BIN}
    tar xzf ${kubectx_cache_file} kubectx
    chmod +x kubectx
  )

  download_if_linux_arm64 ${kubens_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubens_${version}_linux_arm64.tar.gz
  download_if_linux_amd64 ${kubens_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubens_${version}_linux_x86_64.tar.gz
  download_if_darwin_arm64 ${kubens_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubens_${version}_darwin_arm64.tar.gz
  download_if_darwin_amd64 ${kubens_key} https://github.com/ahmetb/kubectx/releases/download/${version}/kubens_${version}_darwin_x86_64.tar.gz
  kubens_cache_file=$(get_cache ${kubens_key})
  (
    cd ${OPT_BIN}
    tar xzf ${kubens_cache_file} kubens
    chmod +x kubens
  )
}

setup_fzf() {
  info "Installing fzf..."
  version=0.42.0
  key="fzf_${version}"

  download_if_linux_arm64 ${key} https://github.com/junegunn/fzf/releases/download/${version}/fzf-${version}-linux_arm64.tar.gz
  download_if_linux_amd64 ${key} https://github.com/junegunn/fzf/releases/download/${version}/fzf-${version}-linux_amd64.tar.gz
  download_if_darwin_arm64 ${key} https://github.com/junegunn/fzf/releases/download/${version}/fzf-${version}-darwin_arm64.zip
  download_if_darwin_amd64 ${key} https://github.com/junegunn/fzf/releases/download/${version}/fzf-${version}-darwin_amd64.zip

  cache_file=$(get_cache ${key})
  case "$(uname -s | tr '[:upper:]' '[:lower:]')" in
    linux)
      tar xzf ${cache_file} -C ${OPT_BIN}
      ;;
    darwin)
      unzip -p ${cache_file} > ${OPT_BIN}/fzf
      chmod +x ${OPT_BIN}/fzf
      ;;
  esac
}

