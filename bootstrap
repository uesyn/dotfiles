  info "Installing rustup-init..."
  rustup_init_path=${OPT_BIN}/rustup-init
  cat <<EOF > ${rustup_init_path}
#!/usr/bin/env sh

tea rustup-init --no-modify-path --default-toolchain nightly --profile default "\$@"
EOF
  chmod +x ${rustup_init_path}
}

setup_mc() {
  info "Installing mc"
  mc_path=${OPT_BIN}/mc
  cat <<EOF > ${mc_path}
#!/usr/bin/env sh

docker run -u $(id -u) --rm -e HOME=${HOME} -w $(pwd) -v ${HOME}:${HOME} -v ${HOME}:${HOME} -i minio/mc:RELEASE.2023-07-21T20-44-27Z "\$@"
EOF
  chmod +x ${mc_path}
}

