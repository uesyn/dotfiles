setup_go() {
  info "Installing go..."
  go_version=1.20.8
  gopls_version=v0.13.2
  go_1_21_version="1.21.1"
  make_tea_command "go" "--silent" "go.dev=${go_version}"
  make_tea_command_with_bin_name "go" "go1.21" "--silent" "go.dev=${go_1_21_version}"
  gopls_path=${OPT_BIN}/gopls
  cat <<EOF > ${gopls_path}
#!/usr/bin/env sh

tea --silent +go.dev=${go_version} go run golang.org/x/tools/gopls@${gopls_version} "\$@"
EOF
  chmod +x ${gopls_path}
}

setup_kubectl() {
  info "Installing kubectl..."
  version=1.28.2
  kubectl_1_28_version=1.28.2
  kubectl_1_27_version=1.27.4
  kubectl_1_26_version=1.26.5
  kubectl_1_25_version=1.25.10
  key="kubectl_${version}"

  download_if_linux_arm64 ${key} https://dl.k8s.io/release/v${version}/bin/linux/arm64/kubectl
  download_if_linux_amd64 ${key} https://dl.k8s.io/release/v${version}/bin/linux/amd64/kubectl
  download_if_darwin_arm64 ${key} https://dl.k8s.io/release/v${version}/bin/darwin/arm64/kubectl
  download_if_darwin_amd64 ${key} https://dl.k8s.io/release/v${version}/bin/darwin/amd64/kubectl

  cache_file=$(get_cache ${key})
  cp ${cache_file} ${OPT_BIN}/kubectl
  chmod +x ${OPT_BIN}/kubectl

  completion_dir=${HOME}/.cache/completion
  mkdir -p ${completion_dir}
  ${OPT_BIN}/kubectl completion zsh > ${completion_dir}/kubectl

  make_tea_command_with_bin_name "kubectl" "kubectl1.28" "--silent" "kubernetes.io/kubectl=${kubectl_1_28_version}"
  make_tea_command_with_bin_name "kubectl" "kubectl1.27" "--silent" "kubernetes.io/kubectl=${kubectl_1_27_version}"
  make_tea_command_with_bin_name "kubectl" "kubectl1.26" "--silent" "kubernetes.io/kubectl=${kubectl_1_26_version}"
  make_tea_command_with_bin_name "kubectl" "kubectl1.25" "--silent" "kubernetes.io/kubectl=${kubectl_1_25_version}"
}

